    "scripts": {
        "build": "tsc",
        "start": "node ./dist/index.js",
        "compile": "yarn run build && yarn run start",
        "postinstall": "npm run install-ui && npm run build-ui",
        "start:dev": "nodemon -e ts  --exec \"yarn run compile\"",
        "install-ui": "cd lib/ui-next && npm i",
        "build-ui": "cd lib/ui-next && npm run build",
        "dev": "concurrently 'npm run dev-server' 'npm run dev-client'",
        "dev-client": "cd lib/ui-next && npm start",
        "dev-server": "nodemon --inspect=5858 server.js",
        "prod-server": "npm run build-ui && npm start",
        "lint": "eslint . --fix"
